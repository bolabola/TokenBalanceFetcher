{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "includeFiles": "dist/public/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}